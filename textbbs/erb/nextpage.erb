<%require 'erb'%>	<% template = ERB.new(File.read('shared/_form.erb')) %>	<%= template.result(binding) %><% page = Page.new %><% hash = page.next_page %><% hash.each do |key, value| %><table>    <tr>        <td class="contribution_name"><%= value['title'].force_encoding("UTF-8")%></td>        <td>投稿者：</td><td class="contribution_title"><%= value['name'].force_encoding("UTF-8")%></td>                        <td><%= value['request_time'].force_encoding("UTF-8")%></td>        <td><a href="javascript:void(0)"onClick="reply('<%= value['title'].force_encoding("UTF-8") %>', '<%=value['textarea'].force_encoding("UTF-8") %>');">[返信]</a></td>    </tr>    <tr>        <td><%= value['textarea'].force_encoding("UTF-8")%></td>    </tr>    <hr></table><% end %><% if page.get_current_page != 1 %>    <a href="/previous">←</a><% end %><a href="/next">→</a>